    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.2",
    "esbuild": "^0.25.8",
    "eslint": "^8.57.1",
    "mocha": "^10.2.0",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1",
    "typescript": "^5.4.3"
